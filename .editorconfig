# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_size = 4
end_of_line = lf
insert_final_newline = true

# 4 space indentation
[*.blade.php]
indent_style = space
indent_size = 4

[*.scss]
intent_style = space
indent_size = 4

[*.vue]
indent_size = 4

[*.js]
indent_size = 4

